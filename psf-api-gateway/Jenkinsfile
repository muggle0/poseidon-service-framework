pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'git@e.coding.net:muggle0/poseidon/poseidon-service-framework.git']])
      }
    }
    stage('compile and build') {
        steps {
            script {
                if (isUnix()) {
                 withMaven(jdk:'jdk8',maven: 'maven', mavenSettingsConfig: '705da47a-85bf-4335-828c-f341e2a60a94') {
                    sh 'mvn clean package deploy -f ./psf-api-gateway'
                 }
                } else {
                  withMaven(jdk:'jdk8',maven: 'maven', mavenSettingsConfig: '705da47a-85bf-4335-828c-f341e2a60a94') {
                    bat 'mvn clean package deploy -f ./psf-api-gateway'
                 }
                }
            }
        }
    }
  }
}